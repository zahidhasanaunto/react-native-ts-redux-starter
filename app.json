{
  "expo": {
    "name": "rnboilerplate",
    "slug": "rnboilerplate",
    "version": "0.0.1",
    "sdkVersion": "36.0.0",
    "description": "React Native App",
    "ios": {
      "bundleIdentifier": "com.rnboilerplate.app",
      "appStoreUrl": "https://itunes.apple.com/us/app/rnboilerplate-app/id1246143230",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.rnboilerplate.app",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.rnboilerplate.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./src/assets/app/icon.png",
    "splash": {
      "image": "src/assets/app/screen.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "src/assets/images/*",
      "src/assets/fonts",
    ],
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#F7F9FC"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}